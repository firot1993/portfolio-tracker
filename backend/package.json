{
  "name": "portfolio-tracker-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:prod": "cross-env NODE_ENV=production tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed:force": "tsx src/db/seed.ts --force",
    "collector": "tsx src/collector/index.ts"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.3",
    "p-limit": "^7.3.0",
    "sql.js": "^1.10.2",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.18",
    "cross-env": "^10.1.0",
    "supertest": "^7.2.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
